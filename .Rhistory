graphsumo = function(dataset, abs, ord, MFV = "vir") {
library(ggplot2)
if (MFV == "male") {
graph <-ggplot(dataset)+
aes(abs,
ord,
fill = abs)+
geom_jitter(size = 1.5,
alpha = 1,
shape = 21)+
geom_violin(alpha = 0.1,
draw_quantiles = c(0.5))+
scale_fill_manual(values = c("black",
"white")
)+
scale_color_manual(values = c("black",
"black"))+
theme_classic()+
labs(x="",
y="")+
theme(legend.position="none",
axis.text = element_text(size = 6))+
stat_summary(fun=mean,
colour="black",
geom="point",
shape = 3)
graph}
else {if (MFV == "female"){
graph <-ggplot(dataset)+
aes(abs,
ord,
fill = abs)+
geom_jitter(size = 1.5,
alpha = 1,
shape = 21)+
geom_violin(alpha = 0.1,
draw_quantiles = c(0.5))+
scale_fill_manual(values = c("white",
"#a00000"))+
scale_color_manual(values = c("a00000",
"black")
)+
theme_classic()+
labs(x="",
y="")+
theme(legend.position="none",
axis.text = element_text(size = 6))+
stat_summary(fun=mean,
colour="black",
geom="point",
shape = 3)
graph} else {
graph <-ggplot(dataset)+
aes(abs,
ord,
fill = abs)+
geom_jitter(size = 1.5,
alpha = 1,
shape = 21)+
geom_violin(alpha = 0.1,
draw_quantiles = c(0.5))+
scale_fill_viridis_d()+
theme_classic()+
labs(x="",
y="")+
theme(legend.position="none",
axis.text = element_text(size = 6))+
stat_summary(fun=mean,
colour="black",
geom="point",
shape = 3)
graph
}
}
return(graph)
}
graphsumo(iris, iris$Species, iris$Petal.Length, "vir")
